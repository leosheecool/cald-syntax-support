{
  "name": "Cald Color Theme",
  "type": "dark",
  "colors": {
      "editor.foreground": "#FFFFFF",
      "editor.background": "#1E1E1E",
      "editor.selectionBackground": "#264F78",
      "editor.lineHighlightBackground": "#333333",
      "editorCursor.foreground": "#FFFFFF",
      "editorWhitespace.foreground": "#FFFFFF30"
  },
  "tokenColors": [
      {
          "scope": "comment.line.number-sign.cald",
          "settings": {
              "foreground": "#6A9955"
          }
      },
      {
          "scope": "keyword.control.cald",
          "settings": {
              "foreground": "#569CD6"
          }
      },
      {
          "scope": "keyword.operator.cald",
          "settings": {
              "foreground": "#C586C0"
          }
      },
      {
          "scope": "constant.language.cald",
          "settings": {
              "foreground": "#DCDCAA"
          }
      },
      {
          "scope": "variable.language.cald",
          "settings": {
              "foreground": "#9CDCFE"
          }
      }
  ]
}
